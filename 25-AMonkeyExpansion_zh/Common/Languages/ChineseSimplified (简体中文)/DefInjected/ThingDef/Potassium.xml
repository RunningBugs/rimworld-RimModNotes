  <ARCCGun_BananaBigBoiTurret.verbs.Verb_Shoot.label>大型钾炮</ARCCGun_BananaBigBoiTurret.verbs.Verb_Shoot.label>
  
  <!-- EN: small potassium turret -->
  <ARCCGun_BananaMiniTurret.label>小型钾枪</ARCCGun_BananaMiniTurret.label>
